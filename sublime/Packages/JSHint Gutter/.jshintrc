{
  // Options
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "freeze": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "onevar": true,
  "plusplus": false,
  "quotmark": true,
  "undef": true,
  "unused": true,
  "strict": true,
  "maxparams": 10,
  "maxdepth": 3,
  "maxlen": 120,

  /*
   * Globals
   */
  "globals": {
    "Ext": true,
    "i18n": true,
    "browser": true,
    "window": true,
    "document": true,
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "afterEach": true,
    "OpusWebUi": true,
    "jasmine": true,
    "spyOn": true,
    "log4javascript": true,
    "l4j": true
  }
}